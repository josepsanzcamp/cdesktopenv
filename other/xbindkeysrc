
# Caja
"caja"
	Mod2+Mod4 + e

# XTerm
"myxterm"
	Mod2+Mod4 + t

# XTerm
"geany"
	Mod2+Mod4 + g

# Vol+
"mypulseaudio up"
	Mod2 + XF86AudioRaiseVolume

# Vol-
"mypulseaudio down"
	Mod2 + XF86AudioLowerVolume

# VolMute
"mypulseaudio mute"
	Mod2 + XF86AudioMute

# Reset
#"mypulseaudio reset"
#	XF86TouchpadToggle

# Backlight+
"mybrightness up"
	Mod2 + XF86MonBrightnessUp

# Backlight-
"mybrightness down"
	Mod2 + XF86MonBrightnessDown

# BacklightMute
"mybrightness mute"
	Mod2 + XF86ScreenSaver

# Display
"mybrightness reset"
	Mod2+Mod4 + p

# Workspace Up
"mydtswitch up"
	Control+Alt + Up

# Workspace Down
"mydtswitch down"
	Control+Alt + Down

# Workspace Right
"mydtswitch right"
	Control+Alt + Right

# Workspace Left
"mydtswitch left"
	Control+Alt + Left

# Move Up
"mydtswitch up2"
	Control+Shift+Alt + Up

# Move Down
"mydtswitch down2"
	Control+Shift+Alt + Down

# Move Right
"mydtswitch right2"
	Control+Shift+Alt + Right

# Move Left
"mydtswitch left2"
	Control+Shift+Alt + Left

# Workspace One
"mydtswitch One"
	Control+Alt + 1

# Workspace Two
"mydtswitch Two"
	Control+Alt + 2

# Workspace Three
"mydtswitch Three"
	Control+Alt + 3

# Workspace Four
"mydtswitch Four"
	Control+Alt + 4

# Workspace Five
"mydtswitch Five"
	Control+Alt + 5

# Workspace Six
"mydtswitch Six"
	Control+Alt + 6

# Screenshoter
"myscreenshot"
