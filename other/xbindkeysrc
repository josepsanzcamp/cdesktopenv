
# Caja
"caja"
	Mod2+Mod4 + e

# XTerm
"myxterm"
	Mod2+Mod4 + t

# XTerm
"geany"
	Mod2+Mod4 + g

# Vol+
"mypulseaudio Up"
	Mod2 + XF86AudioRaiseVolume

# Vol-
"mypulseaudio Down"
	Mod2 + XF86AudioLowerVolume

# VolMute
"mypulseaudio Mute"
	Mod2 + XF86AudioMute

# Reset
#"mypulseaudio Reset"
#	XF86TouchpadToggle

# Backlight+
"mybrightness Up"
	Mod2 + XF86MonBrightnessUp

# Backlight-
"mybrightness Down"
	Mod2 + XF86MonBrightnessDown

# BacklightMute
"mybrightness Mute"
	Mod2 + XF86ScreenSaver

# Display
"mybrightness Reset"
	Mod2+Mod4 + p

# Workspace Up
"mydtswitch Up"
	Control+Alt + Up

# Workspace Down
"mydtswitch Down"
	Control+Alt + Down

# Workspace Right
"mydtswitch Right"
	Control+Alt + Right

# Workspace Left
"mydtswitch Left"
	Control+Alt + Left

# Move Up
"mydtswitch Up2"
	Control+Shift+Alt + Up

# Move Down
"mydtswitch Down2"
	Control+Shift+Alt + Down

# Move Right
"mydtswitch Right2"
	Control+Shift+Alt + Right

# Move Left
"mydtswitch Left2"
	Control+Shift+Alt + Left

# Workspace One
"mydtswitch One"
	Control+Alt + 1

# Workspace Two
"mydtswitch Two"
	Control+Alt + 2

# Workspace Three
"mydtswitch Three"
	Control+Alt + 3

# Workspace Four
"mydtswitch Four"
	Control+Alt + 4

# Workspace Five
"mydtswitch Five"
	Control+Alt + 5

